#to fine leap year or not
import java.util.*;

class HelloWorld {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        System.out.print("enter year to fine leap");
        int a = sc.nextInt();
        if(a%4==0){
            System.out.print("leap year");
            
        }else {
            
            System.out.print("no");
        }
        
    
        
    }
}
